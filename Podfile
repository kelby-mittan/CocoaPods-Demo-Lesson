# Uncomment the next line to define a global platform for your project
# platform :ios, '9.0'

target 'CocoaPods-Demo' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!

  # Pods for CocoaPods-Demo
  pod 'Alamofire' # not specifying the version number will install the most recent version of the library
  
  # Snapkit is an Autolayout third party library
  pod 'SnapKit'
  
  # Kingfisher is a photo processing library
  pod 'Kingfisher'
  
  
  target 'CocoaPods-DemoTests' do
    inherit! :search_paths
    # Pods for testing
  end

end
